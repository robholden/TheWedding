{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=data.db"
  },
  "App": {
    "DevKey": "supersecret"
  },
  "JWT": {
    "Issuer": "Score Caddy",
    "IssuerSigningKey": "ASuperSecretKeyWhichIsAtleast256BytesLong"
  },
  "Origins": "https://scorecaddy.app, capacitor://localhost",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
